if &compatible
  set nocompatible               " Be iMproved
endif

set runtimepath+=/home/locutor/.dotfiles/dein/repos/github.com/Shougo/dein.vim

if dein#load_state('/home/locutor/.dotfiles/dein')
  call dein#begin('/home/locutor/.dotfiles/dein')

  call dein#add('/home/locutor/.dotfiles/dein/repos/github.com/Shougo/dein.vim')

  call dein#end()
  call dein#save_state()
endif

if dein#check_install()
  call dein#install()
endif

